{
  "name": "next-starter",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/tonyfromundefined/next-starter",
  "author": "Tony Won <jeehyukwon@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "node ./index.js",
    "prebuild": "rimraf ./dist",
    "build": "next build ./src",
    "postbuild": "parallel-webpack",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:stage": "NODE_ENV=production sls deploy --stage stage",
    "deploy:prod": "NODE_ENV=production sls deploy --stage prod",
    "lint": "tslint --project '.'",
    "pregenerate": "get-graphql-schema `dotenv -e .env.development -p NEXT_APP_GRAPHQL_ENDPOINT` > ./src/generated/schema.graphql",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "aws-serverless-express": "^3.3.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "encoding": "^0.1.12",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "mobx": "^5.10.1",
    "mobx-react-lite": "^1.4.0",
    "next": "^8.1.0",
    "nocache": "^2.1.0",
    "open-color": "^1.7.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-apollo-hooks": "^0.4.5",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@graphql-codegen/cli": "1.1.3",
    "@graphql-codegen/typescript": "1.1.3",
    "@graphql-codegen/typescript-operations": "1.1.3",
    "@graphql-codegen/typescript-react-apollo": "1.1.3",
    "@types/express-serve-static-core": "^4.16.7",
    "@types/lodash": "^4.14.134",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.16",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "dotenv": "^8.0.0",
